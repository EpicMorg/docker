services:
  app:
    image: "quay.io/epicmorg/confluence:{{ item }}"
    build:
      context: .
  app-jdk17:
    image: "quay.io/epicmorg/confluence:{{ item }}-jdk17"
    build:
      context: .
  app-jdk21:
    image: "quay.io/epicmorg/confluence:{{ item }}-jdk21"
    build:
      context: .
      dockerfile: Dockerfile.jdk21