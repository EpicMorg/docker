services:
  app:
    image: "epicmorg/confluence:{{ item }}"
    build:
      context: .
  app-jdk8:
    image: "epicmorg/confluence:{{ item }}-jdk8"
    build:
      context: .
  app-jdk11:
    image: "epicmorg/confluence:{{ item }}-jdk11"
    build:
      context: .
      dockerfile: Dockerfile.jdk11
